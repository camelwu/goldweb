<script type="text/javascript">
$(function(){
	var price = <{$info.price2}>;
	var go_type = <{$info.go_type}>;
	var go_time = '<{$info.go_time}>';
	var obj = {<{foreach name=price from=$prices key=time item=data}>"<{$time}>":<{$data}><{if $smarty.foreach.price.last}><{else}>,<{/if}><{/foreach}>}; 
	new Calender({id:'cd_date',time:obj});
	//new Calender({id:'cd_date',isSelect:0,price:price,go_type:go_type,go_time:go_time});
	$(".jq_price .price_exp").mouseenter(function(){
		$(this).find("div").show();
	})
	$(".jq_price .price_exp").mouseleave(function(){
		$(this).find("div").hide();
	})	
	$(".select_arrow").click(function(){		
		$(this).toggleClass("se_ar_on");
		$(".svo_box").slideToggle(200);
	});	
	$(".cd_info_title li").click(function(){//商品详情——订单导航
		//$(this).siblings().removeClass("cd_on");
	   // $(this).addClass("cd_on");	
		var cdit_num=$(".cd_info_title li").index(this)+1;
		var cdit_top=$(".cd_t"+cdit_num).offset().top-60;
		 $("html, body").animate({ scrollTop: cdit_top},300);		
	});	
	$(".trip_left ul li").click(function(){//商品详情——天数导航
		var cdit_num=$(".trip_left ul li").index(this);
		var cdit_top=$(".frt_ul").eq(cdit_num).offset().top-50;
		$("html, body").animate({ scrollTop: cdit_top},300);
				
	});
	$(window).scroll(function(){	
	var vv=document.documentElement.scrollTop||document.body.scrollTop; //解决谷歌等不识别document.documentElement.scrollT
	$(".of_info .info_title").each(function(i){
		var cd_t=$(this).offset().top-80;
		if(cd_t<vv){
			$(".cd_info_title li").eq(i).siblings().removeClass("cd_on");
			$(".cd_info_title li").eq(i).addClass("cd_on");			
			}		
		})
	$(".trip_right .frt_ul").each(function(x){
		var frtu=$(this).offset().top-80;
		if(frtu<vv){
			$(".trip_left ul li").eq(x).siblings().removeClass("trl_on");
			$(".trip_left ul li").eq(x).addClass("trl_on");		
			}		
		})
	})
	$(".cd_info_title").pin({		
    containerSelector: ".cd_info"
	})
	$(".trip_left ul").pin({		
    containerSelector: ".trip_box"
	})
	$(".cd_slideBox").slide({mainCell:".bd ul",effect:"fold",autoPlay:true,easing:"easeOutCirc",delayTime:800});	
	
});
</script>
