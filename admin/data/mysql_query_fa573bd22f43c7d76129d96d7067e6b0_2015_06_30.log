INSERT INTO cg_scenic (types, title, ctype, info_id, cid, aid, brand, item, size, stock, price, price1, word, info, op_type, url, op_user, userid, bid) VALUES ('2', '阿联酋旅游签', '152', '', '2', '132', '5', '59天', '30天', '', '900', '100', '个人身份证明
<p class=\"MsoNormal\">
	照片
</p>
<p class=\"MsoNormal\">
	两寸白底彩色照片2张，在照片后面签名
